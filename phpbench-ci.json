{
    "$schema": "./vendor/phpbench/phpbench/phpbench.schema.json",
    "runner.bootstrap": "vendor/autoload.php",
    "runner.path": "benchmarks",
    "runner.file_pattern": "*Bench.php",
    "runner.iterations": 1,
    "runner.revs": 10,
    "runner.warmup": 0,
    "runner.time_unit": "microseconds",
    "runner.retry_threshold": 2,
    "runner.env_enabled_providers": ["git", "uname"],
    "report.generators": {
        "default": {
            "generator": "expression",
            "cols": [
                "benchmark",
                "subject",
                "set",
                "mean",
                "stdev",
                "mem_peak"
            ]
        },
        "ci": {
            "generator": "expression",
            "cols": [
                "benchmark",
                "subject",
                "mean",
                "mem_peak",
                "mode"
            ]
        }
    },
    "storage.xml_storage_path": ".phpbench"
}