{
    "name": "uniacid/sortedlinkedlist",
    "description": "A type-safe, automatically-sorted linked list data structure for PHP",
    "homepage": "https://github.com/uniacid/sortedlinkedlist",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SortedLinkedList Contributors"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpbench/phpbench": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "SortedLinkedList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SortedLinkedList\\Tests\\": "tests/",
            "SortedLinkedList\\Benchmarks\\": "benchmarks/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --coverage-text --coverage-html=coverage",
        "analyse": "phpstan analyse",
        "bench": "phpbench run --report=default",
        "bench-compare": "phpbench run --report=compare",
        "bench-memory": "phpbench run --report=memory",
        "check": [
            "@test",
            "@analyse"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}