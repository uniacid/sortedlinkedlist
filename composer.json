{
    "name": "uniacid/sortedlinkedlist",
    "description": "A type-safe, automatically-sorted linked list data structure for PHP",
    "homepage": "https://github.com/uniacid/sortedlinkedlist",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "sorted-list",
        "linked-list",
        "data-structure",
        "collection",
        "type-safe",
        "psr-12",
        "sorting",
        "binary-search",
        "iterator"
    ],
    "authors": [
        {
            "name": "SortedLinkedList Contributors",
            "homepage": "https://github.com/uniacid/sortedlinkedlist/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/uniacid/sortedlinkedlist/issues",
        "source": "https://github.com/uniacid/sortedlinkedlist",
        "docs": "https://github.com/uniacid/sortedlinkedlist#readme"
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2",
        "phpdocumentor/phpdocumentor": "^3.4",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "SortedLinkedList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SortedLinkedList\\Tests\\": "tests/",
            "SortedLinkedList\\Benchmarks\\": "benchmarks/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --coverage-text --coverage-html=coverage",
        "analyse": "phpstan analyse",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "bench": "phpbench run --report=default",
        "bench-compare": "phpbench run --report=compare",
        "bench-memory": "phpbench run --report=memory",
        "docs": "phpdoc -c phpdoc.dist.xml",
        "docs-serve": "php -S localhost:8080 -t docs/api",
        "check": [
            "@test",
            "@analyse",
            "@cs-check"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
